class CuentaBancaria:
    def __init__(self, titular: str, numero_cuenta: str, saldo: float = 0.0):
        self.titular = titular
        self.numero_cuenta = numero_cuenta
        self.saldo = saldo

    def depositar(self, monto: float):
        if monto <= 0:
            print("El monto a depositar debe ser positivo.")
            return
        self.saldo += monto
        print(f"Depósito exitoso: +${monto}. Saldo actual: ${self.saldo}")
    def retirar(self, monto: float):
        if monto <= 0:
            print("El monto a retirar debe ser positivo.")
            return
        if monto > self.saldo:
            print("Fondos insuficientes. No puedes retirar más de lo que tienes.")
            return
        self.saldo -= monto
        print(f"Retiro exitoso: -${monto}. Saldo actual: ${self.saldo}")
    def mostrar_info(self):
        print(f"Titular: {self.titular} | Cuenta: {self.numero_cuenta} | Saldo: ${self.saldo}")
if __name__ == "__main__":
    cuenta = CuentaBancaria("Ana Pérez", "123-456", 500.0)
    cuenta.mostrar_info()
    cuenta.depositar(200)
    cuenta.retirar(100)
    cuenta.retirar(700)
    cuenta.mostrar_info()
